{
    "name": "humamkerdiah/fcm-notifications",
    "description": "A Laravel package for sending Firebase Cloud Messaging (FCM) push notifications with easy integration and configuration.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "firebase",
        "fcm",
        "notifications",
        "push",
        "push notifications",
        "FCM Laravel",
        "firebase notifications",
        "send push",
        "laravel push",
        "FCM integration"
    ],
    "autoload": {
        "psr-4": {
            "Humamkerdiah\\FcmNotifications\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Humam Kerdiah",
            "email": "humamkerdia58@gmail.com"
        }
    ],
    "support": {
        "email": "humamkerdia58@gmail.com",
        "issues": "https://github.com/humamkerdiah/fcm-notifications/issues",
        "source": "https://github.com/humamkerdiah/fcm-notifications"
    },
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "firebase/php-jwt": "^6.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Humamkerdiah\\FcmNotifications\\FcmNotificationsServiceProvider"
            ],
            "aliases": {
                "FcmNotification": "Humamkerdiah\\FcmNotifications\\Facades\\FcmNotification"
            }
        }
    },
    "minimum-stability": "stable"
}

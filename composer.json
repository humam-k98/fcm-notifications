{
    "name": "humamkerdiah/fcm-notifications",
    "description": "Firebase Cloud Messaging (FCM) notifications package for Laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Humamkerdiah\\FcmNotifications\\": "src/"
        }
    },
    "authors": [
        {
            "name": "humam kerdiah",
            "email": "humamkerdiah97@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "firebase/php-jwt": "^6.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Humamkerdiah\\FcmNotifications\\FcmNotificationsServiceProvider"
            ],
            "aliases": {
                "FcmNotification": "Humamkerdiah\\FcmNotifications\\Facades\\FcmNotification"
            }
        }
    },
    "minimum-stability": "stable"
}
